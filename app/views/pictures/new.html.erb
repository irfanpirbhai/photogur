<h1>Add a picture</h1>
<!-- <form action="/pictures/update" method="POST"> -->
  
<%= form_for @picture do |f| %>
  <p>
    <label>Artist</label><br>
    <%= f.text_field "artist" %>
  <p>
    <label>Title</label><br>
    <%= f.text_field "title" %>
  </p>
    <label>URL</label><br>
    <%= f.text_field "url" %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
</form>

<% end %>

